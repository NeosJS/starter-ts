{
  "name": "@neosjs/template",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.5.0",
  "description": "_description_",
  "author": "听着情歌流泪",
  "license": "MIT",
  "homepage": "https://docs.neosjs.com",
  "keywords": [
    "neosjs",
    "neos"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "release && npm publish",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@neosjs/eslint-config": "^1.1.10",
    "@types/node": "^20.14.11",
    "eslint": "^9.7.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.7",
    "pnpm": "^9.5.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
